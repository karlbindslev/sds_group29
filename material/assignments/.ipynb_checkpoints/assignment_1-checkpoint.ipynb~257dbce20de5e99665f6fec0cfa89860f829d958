    "# [Answer to Ex. 6.1.4]\n",
    "import pandas as pd\n",
    "\n",
    "url = \"https://www1.ncdc.noaa.gov/pub/data/ghcn/daily/by_year/1864.csv.gz\"\n",
    "vejr = pd.read_csv(url, compression = \"gzip\", header = None).drop[columns=[4,5,6,7]]\n",
    "print(vejr)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "vejr1 = vejr.copy()\n",
    "vejr1[\"datetime\"] = pd.to_datetime(vejr.date.astype(str), format = \"%Y%m%d\")\n",
    "\n",
    "vejr1 = vejr1.drop(columns = [\"date\"])\n",
    "\n",
    "vejr1['month'] = pd.DatetimeIndex(vejr1['datetime']).month\n",
    "\n",
    "vejr1 = vejr.drop(columns=[4,5,6,7])\n",
    "\n",
    "COLS = [\"stat_id\", \"date\", \"obs_type\", \"value\"]\n",
    "vejr1.columns = COLS\n",
    "\n",
    "vejr1[[\"value\"]] = vejr1[[\"value\"]].astype(float)\n",
    "vejr1[[\"stat_id\", \"obs_type\"]] = vejr1[[\"stat_id\", \"obs_type\"]].astype(str)\n"
   ]
